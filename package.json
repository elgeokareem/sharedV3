{
  "name": "@insightt/insightt-shared",
  "version": "1.1.0",
  "description": "Shared code for Insightt",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "npx husky install",
    "test": "jest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "insightt/alacret",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "moment": "^2.24.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules)/"
    ],
    "testRegex": [
      ".test.ts$",
      ".test.tsx$"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.8",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^39.4.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
