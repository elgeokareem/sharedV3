{
  "name": "@cobuildlab/create-nodejs-app",
  "version": "1.0.0",
  "description": "Starter for a npm package",
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "npx husky install",
    "test": "jest"
  },
  "author": "Gressia",
  "license": "ISC",
  "dependencies": {
    "eslint": "^8.8.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules)/"
    ],
    "testRegex": [
      ".test.ts$",
      ".test.tsx$"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.8.2",
    "husky": "^7.0.4"
  }
}
